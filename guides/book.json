{
  "author": "Aeneas Rekkas",
  "gitbook": ">=3.2.3",
  "plugins": [
    "edit-link",
    "summary",
    "etoc",
    "ga",
    "sitemap",
    "versions"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/ory/hydra/tree/master/docs",
      "label": "Edit this page on GitHub"
    },
    "etoc": {
      "h2lb": 2,
      "mindepth": 2,
      "maxdepth": 5,
      "notoc": false
    },
    "ga": {
      "token": "UA-71865250-1"
    }
    "sitemap": {
      "hostname": "http://mybook.com/"
    },
    "versions": {
      "type": "tags"
    }
  }
}