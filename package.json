{
  "name": "docusaurus-template",
  "version": "0.0.0",
  "private": true,
  "prettier": "ory-prettier-styles",
  "config": {
    "prettierTarget": "{code-examples/**,docs/**,docs/,scripts/,static/,contrib/,src/,src/**/,}*.{md,mdx,json,js,css,html,ts,tsx}"
  },
  "scripts": {
    "gen": "npm-run-all gen:*",
    "docusaurus": "docusaurus",
    "gen:api": "mkdir -p .static && cp ../spec/api.json src/static/api.json",
    "gen:config": "node src/scripts/config.js config.js",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "serve": "docusaurus serve",
    "format": "prettier --write ${npm_package_config_prettierTarget}",
    "format:check": "prettier --check ${npm_package_config_prettierTarget}",
    "test": "playwright test",
    "test:unit": "jest tests/jest --maxConcurrency=8 --bail 1"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.9",
    "@docusaurus/core": "2.0.0-beta.15",
    "@docusaurus/plugin-client-redirects": "^2.0.0-beta.15",
    "@docusaurus/plugin-content-docs": "2.0.0-beta.15",
    "@docusaurus/plugin-content-pages": "2.0.0-beta.15",
    "@docusaurus/plugin-sitemap": "2.0.0-beta.15",
    "@docusaurus/theme-classic": "2.0.0-beta.15",
    "@docusaurus/theme-search-algolia": "2.0.0-beta.15",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@octokit/rest": "^18.12.0",
    "@ory/client": "0.0.1-alpha.74",
    "@rjsf/core": "^3.2.1",
    "@rjsf/material-ui": "^3.2.1",
    "axios": "^0.25.0",
    "classnames": "2.3.1",
    "clsx": "^1.1.1",
    "file-loader": "6.2.0",
    "json-loader": "^0.5.7",
    "json-schema-faker": "^0.5.0-rcv.41",
    "mermaid": "8.13.8",
    "net": "^1.0.2",
    "node-fetch": "2.6.7",
    "ory-prettier-styles": "1.1.2",
    "parser-front-matter": "1.6.4",
    "prettier": "2.3.2",
    "prism-react-renderer": "1.2.1",
    "prismjs": "1.27.0",
    "ramda": "^0.27.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "redocusaurus": "0.6.0",
    "remark-admonitions": "1.2.1",
    "remarkable": "2.0.1",
    "semver": "^7.3.5",
    "tls": "^0.0.1",
    "url-loader": "4.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "@swc/core-darwin-x64": "^1.2.139"
  },
  "devDependencies": {
    "@playwright/test": "^1.18.1",
    "@swc/core": "^1.2.139",
    "@swc/jest": "^0.2.17",
    "@types/jest": "^27.4.0",
    "axios-retry": "^3.2.4",
    "fast-xml-parser": "^4.0.2",
    "jasmine-fail-fast": "^2.0.1",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "path-to-regexp": "^6.2.0",
    "raw-loader": "^4.0.2",
    "text-runner": "^5.0.0",
    "wait-on": "^6.0.0",
    "yaml": "^1.10.2"
  }
}
